{
  "expo": {
    "name": "SoulSetu-SocializingApp",
    "slug": "SoulSetu-SocializingApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "soulsetusocializingapp",
    "userInterfaceStyle": "automatic",
    "androidNavigationBar": {
      "backgroundColor": "#0a0a0a"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "We use Bluetooth to discover nearby users.",
        "UIBackgroundModes": [
          "bluetooth-central",
          "bluetooth-peripheral"
        ],
        "NSLocationWhenInUseUsageDescription": "We need your location to show your neighborhood on the map."
      },
      "bundleIdentifier": "com.anonymous.SoulSetuSocializingApp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyANJwejlqBRgFTT-5s3xNVbVOkxyPBWGz4"
        }
      },
      "package": "com.anonymous.SoulSetuSocializingApp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-video",
      "expo-font",
      "expo-sqlite",
      [
        "react-native-ble-plx",
        {
          "isBackgroundEnable": true,
          "modes": [
            "central"
          ],
          "neverForLocation": false,
          "bluetoothAlwaysPermission": "Allow SoulSetu to discover nearby devices"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}